{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "debug",
      "type": "node",
      "request": "launch",
      "args": ["./src/app.ts"],
      "runtimeArgs": ["--nolazy", "-r", "ts-node/register", "-r", "dotenv/config"],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "resolveSourceMapLocations": ["${workspaceFolder}/src", "!${workspaceFolder}/node_modules/**"],
      "protocol": "inspector"
    }
  ]
}
